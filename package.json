{
  "name": "relative-goto",
  "displayName": "relative goto",
  "description": "Goto / Jump to a Relative line",
  "version": "0.5.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/Enkel-Digital/relative-goto.git"
  },
  "author": "JJ",
  "publisher": "EnkelDigital",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Enkel-Digital/relative-goto/issues"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "engines": {
    "vscode": "^1.45.0"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "theme": "light",
    "color": "#ffa600"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "shortcuts",
    "relative",
    "goto",
    "lines",
    "jump"
  ],
  "activationEvents": [
    "onCommand:relative-goto.goto",
    "onCommand:relative-goto.select"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "relative-goto.goto",
        "title": "Goto relative line"
      },
      {
        "command": "relative-goto.select",
        "title": "Select using relative line Goto/Jump"
      }
    ],
    "keybindings": [
      {
        "command": "relative-goto.goto",
        "key": "alt+g",
        "mac": "cmd+g"
      },
      {
        "command": "relative-goto.select",
        "key": "alt+s",
        "mac": "cmd+s"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.45.1",
    "@types/node": "^14.0.5",
    "eslint": "^7.1.0",
    "@typescript-eslint/parser": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "typescript": "^3.9.3"
  }
}
